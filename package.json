  "scripts": {
    "css.lint": "stylelint --config build/.stylelintrc --syntax scss \"scss/**/*.scss\" --cache --cache-location \"build/.stylelintcache/\"",
    "css.compile": "node-sass --output-style expanded --source-map true --source-map-contents true --precision 6 scss/main.scss css/main.css",
    "css.prefix": "postcss --config build/postcss.config.js --replace \"css/main.css\" \"!css/*.min.css\"",
    "css.minify": "cleancss --level 1 --source-map --source-map-inline-sources --output css/main.min.css css/main.css",
    "css.with.lint": "clear && npm run css.lint && npm run css.compile && npm run css.prefix && npm run css.minify",
    "css": "clear && npm run css.compile && npm run css.prefix && npm run css.minify",
    "css.watch": "watch 'npm run css' scss/",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "yarn run dev",
    "test": "echo 'No tests.'; exit 0;",
    "build": "node build/build.js"
